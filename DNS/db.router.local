;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	servidor.router.local. root.router.local. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	        IN	NS	servidor.router.local.
servidor	IN	A	10.10.10.1
equipo01        IN      A       10.10.10.7
server	        IN	CNAME	servidor
